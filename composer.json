{
  "name": "kussie/command-database-logger",
  "autoload": {
    "psr-4": {
      "Kussie\\CommandDatabaseLogger\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Ben Kuskopf",
      "email": "ben@kussie.com.au"
    }
  ],
  "require": {
    "php": ">=7.4",
    "illuminate/database": ">=8.0",
    "illuminate/support": ">=8.0",
    "spatie/laravel-package-tools": "^1.9.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "nunomaduro/collision": "^6.0",
    "orchestra/testbench": "^7.0",
    "phpunit/phpunit": "^9.5",
    "pestphp/pest": "^1.21",
    "pestphp/pest-plugin-laravel": "^1.1"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kussie\\CommandDatabaseLogger\\CommandLoggerServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
